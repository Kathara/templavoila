{
	"name": "templavoila/templavoila",
	"type": "typo3-cms-extension",
	"description": "One of many availabe template engines for TYPO3 CMS",
	"homepage": "http://forge.typo3.org/projects/extension-templavoila/",
	"license": ["GPL-2.0+"],
	"version": "2.0.4",
	"autoload": {
		"psr-4": {
			"Extension\\Templavoila\\": "Classes/"
		},
		"classmap": [
			"cm1/",
			"mod2/",
			"pi1/"
		]
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		}
	],
	"require": {
		"php": ">=5.5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8.0",
		"mikey179/vfsStream": "1.6.0"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"mkdir -p .Build/Web/typo3conf/ext/",
			"[ -L .Build/Web/typo3conf/ext/templavoila ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/templavoila"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
